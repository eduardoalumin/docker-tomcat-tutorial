<script type="text/javascript">
      $(document).ready(function() {

        $(".arrow").addClass("arrowDown");
        $(".infoCobertura").addClass("infoCerrada");

        $(".arrow").click(function(){

          var informacionCobertura = $(this).parent().parent().prev().children(".infoCobertura");

          if(informacionCobertura.hasClass("infoAbierta")){
            $(informacionCobertura).removeClass("infoAbierta").addClass("infoCerrada");
            $(this).removeClass("arrowUp").addClass("arrowDown");
          }

          else if(informacionCobertura.hasClass("infoCerrada")) {
            $(".infoCobertura").removeClass("infoAbierta").addClass("infoCerrada");
            $(".arrow").removeClass("arrowUp").addClass("arrowDown");
            $(informacionCobertura).removeClass("infoCerrada").addClass("infoAbierta");
            $(this).removeClass("arrowDown").addClass("arrowUp");
          }

          $(".infoAbierta").slideDown(400), "swing";
          $(".infoCerrada").slideUp(200), "swing";

        });

      });
    </script>
